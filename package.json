{
  "type": "module",
  "scripts": {
    "save": "git add . && git commit -m \"patch\" && git push",
    "start": "run-p start:hmr",
    "start:hmr": "node --loader ts-node/esm ./hmr/hmr-server.ts --dir \"./src\"",
    "build": "run-s build:bundle",
    "build:watch": "npm run bundle:watch",
    "build:bundle": "node --loader ts-node/esm ./runBundleScript.ts",
    "build:hmr": "node hmr/runBundleScript.hmr.mjs",
    "save:dist": "npm run patch:version && npm run build:dist && cd ../dist && git add . && git commit -m \"dist patch\" && git push",
    "_build:js": "tsc -p tsconfig.build.json",
    "_build:js:watch": "tsc -p tsconfig.build.json --watch",
    "bundle:watch": "nodemon --exec 'npm run build:bundle' --watch src/."
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "taggedjs": "^2.8.24",
    "taggedjs-cli": "^0.1.19",
    "taggedjs-dump": "^0.1.6"
  }
}
