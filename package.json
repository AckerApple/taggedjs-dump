{
  "scripts": {
    "save": "git add . && git commit -m \"patch\" && git push",
    "start": "npm run build:js:watch & npm run start:hmr",
    "start:hmr": "dir=assets/js node tagged-hmr.js",
    "build": "npm run build:js && npm run build:hmr",
    "build:bundle": "node runBundleScript.js",
    "build:hmr": "node hmr/runBundleScript.js",
    "build:js": "tsc -p tsconfig.json",
    "build:js:watch": "tsc -p tsconfig.json --watch"
  },
  "devDependencies": {
    "express": "^4.18.2",
    "serve-static": "^1.15.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "ws": "^8.15.1"
  },
  "dependencies": {
    "taggedjs": "github:ackerapple/taggedjs",
    "taggedjs-common": "github:ackerapple/taggedjs-common"
  }
}
